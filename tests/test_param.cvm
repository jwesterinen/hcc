foo	entry	$$1
	load	par,0		; a
	load	con,1
	alu	-		; -
	store	par,1		; b
	pop			; clear stack
	load	par,1		; b
	store	gbl,0		; save result
	return			; RETURN
	return			; end of function
$$1	equ	0		; foo
main	entry	$$2
	load	con,1
	load	con,2
	call	2,foo
	pop			; pop argument
	pop			; pop argument
	load	gbl,0		; push result
	store	lcl,0		; x
	pop			; clear stack
	return			; end of function
$$2	equ	1		; main
	end	1, main
